{
    "name": "garbuzivan/laravel-generator-package",
    "description": "Generator the composer package for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Ivan Garbuz",
            "email": "garbuzivan90@ya.ru"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": ">=1.3",
        "laravel/framework": "^8.0",
        "facade/ignition": "^2.3.6",
        "orchestra/testbench": "^6.0"
    },
    "require": {
        "psr/container": "^1.0",
        "webmozart/assert": "^1.9",
        "psr/event-dispatcher": "^1.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GarbuzIvan\\LaravelGeneratorPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GarbuzIvan\\LaravelGeneratorPackage\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GarbuzIvan\\LaravelGeneratorPackage\\ServiceProvider"
            ]
        }
    }
}
